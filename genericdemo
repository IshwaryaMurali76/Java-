
public class GenericClassDemo<T>
{
private T t;
public void setValue(T t)
{
this.t=t;
}
public T getValue()
{
return t;
}
public static void main(String[]args)
{
GenericClassDemo<Integer> iobj=new GenericClassDemo<Integer>();
iobj.setValue(3);
System.out.println(iobj.getValue());
GenericClassDemo<Integer> vobj=new GenericClassDemo<Integer>();
vobj.setValue(3);
System.out.println(iobj.getValue());
GenericClassDemo<Integer> uobj=new GenericClassDemo<Integer>();
uobj.setValue(199095);
System.out.println(iobj.getValue());
GenericClassDemo<String> sobj=new GenericClassDemo<String>();
sobj.setValue("elasa");
System.out.println(sobj.getValue());
}
}
