import java.io.*;
import java.lang.*;
class prime
{
	public static void main(String ars[])
	{
	int num=33, i=2;
	boolean flag=false;
	while(i<=num/2)
	if(num%i==0)
	{
		flag=true;
		break;
	}
	++i;
	
	if (!flag)
		System.out.println(num+"is a prime number");
		else
			System.out.println(num+"is a not prime number");
}
}
